<div class="main">
  <div class="card w-50 bg-light" style="margin: 0 auto; float: none; margin-top: 35px; margin-bottom: 20px">
    <div class="card-body">
      <%= render 'errors/error', model: @banner %>
      <%= form_for @banner, url: admin_banner_path(@banner), method: :patch do |f| %>
        <%= render partial: 'form', locals: {f: f, banner: @banner} %>
      <% end %>
    </div>
  </div>
</div>