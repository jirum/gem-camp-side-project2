<div class="main">
  <div class="card w-50 bg-light" style="margin: 0 auto; float: none; margin-top: 35px; margin-bottom: 20px">
    <div class="card-body">
      <%= render 'errors/error', model: @news_ticker %>
      <%= form_for @news_ticker, url: admin_news_ticker_path(@news_ticker), method: :patch do |f| %>
        <%= render partial: 'form', locals: {f: f, news_ticker: @news_ticker} %>
      <% end %>
    </div>
  </div>
</div>